      "checkConstraints": {
        "chk_name": {
          "name": "chk_name",
          "value": "length(\"pb_projects\".\"name\") > 1 AND length(\"pb_projects\".\"name\") <= 125"
        },
        "chk_status": {
          "name": "chk_status",
          "value": "\"pb_projects\".\"status\" IN ('planning', 'building', 'complete')"
        },
        "chk_dateCompleted": {
          "name": "chk_dateCompleted",
          "value": "(\"pb_projects\".\"status\" == 'complete' AND \"pb_projects\".\"date_completed\" IS NOT NULL) OR (\"pb_projects\".\"status\" != 'complete' AND \"pb_projects\".\"date_completed\" IS NULL)"
        },
        "chk_rating": {
          "name": "chk_rating",
          "value": "(\"pb_projects\".\"status\" == 'complete' AND \"pb_projects\".\"rating\" IS NOT NULL AND \"pb_projects\".\"rating\" >= 1 AND \"pb_projects\".\"rating\" <= 5) OR (\"pb_projects\".\"status\" != 'complete' AND \"pb_projects\".\"rating\" IS NULL)"
        },
        "chk_recommend": {
          "name": "chk_recommend",
          "value": "(\"pb_projects\".\"status\" == 'complete' AND \"pb_projects\".\"recommend\" IS NOT NULL) OR (\"pb_projects\".\"status\" != 'complete' AND \"pb_projects\".\"recommend\" IS NULL)"
