{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./tsconfig.base.json",
	"display": "Shared",
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"isolatedModules": true,
		"noEmit": true,
		"allowImportingTsExtensions": true
	}
}
